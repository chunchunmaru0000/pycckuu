из msvcrt черпать
	printf как гойда некого,
	scanf как начертать некого;

зов гойда "%lld\н" (0 и 0);
зов гойда "%lld\н" (12 и 0);
зов гойда "%lld\н" (0 и 12);
зов гойда "%lld\н" (12 и 12);

зов гойда "%lld\н" (0 или 0);
зов гойда "%lld\н" (12 или 0);
зов гойда "%lld\н" (0 или 12);
зов гойда "%lld\н" (12 или 12);