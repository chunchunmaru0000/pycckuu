из "msvcrt" черпать "printf" как "гойда" некого;
из "msvcrt" черпать "scanf" как "начертать" некого;

из "sqlite3" черпать "sqlite3_open" как "откр";
из "sqlite3" черпать "sqlite3_close" как "закр";
из "sqlite3" черпать "sqlite3_prepare_v2" как "подготовить";
из "sqlite3" черпать "sqlite3_step" как "шаг";
из "sqlite3" черпать "sqlite3_finalize" как "конец";
из "sqlite3" черпать "sqlite3_column_int64" как "кч64";
из "sqlite3" черпать "sqlite3_column_text" как "кстр";

пусть бд будет "бд.db";
зов гойда "%s\н" бд;

пусть подкл будет 0;
зов откр бд ук подкл;
зов гойда "[ БД ПОДКЛЮЧЕНА ]\н";

пусть команда будет 0;
пусть создать_таблицу будет 
"create table if not exists asdf(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	name TEXT, 
	age INTEGER
)";

зов гойда "[ НАЧАЛО ПОДГОТОВКИ КОМАНДЫ ] %s\н" создать_таблицу;
пусть байтов будет -1;
зов подготовить подкл создать_таблицу байтов ук команда 0;
зов гойда "[ КОНЕЦ  ПОДГОТОВКИ КОМАНДЫ ]\н";
зов шаг команда;
зов гойда "[ ШАГ СДЕЛАН]";

зов закр подкл;
зов гойда "[ БД ЗАКРЫТА ]\н";


Ё
пусть негр будет "______________________\н";
пусть негр + 1 например 'N';
пусть негр + 2 например 'I';
пусть негр + 3 например 'G';
пусть негр + 4 например 'G';
пусть негр + 5 например 'A';
пусть негр + 7 например 'Н';
пусть негр + 9 например 'Е';
пусть негр + 11 например 'Г';
пусть негр + 13 например 'Р';
зов гойда негр;


пусть федкин например "гандон";
зов гойда "федкин это %s\н" федкин;
пусть б равно 4;
пусть с будет федкин + б;
зов гойда "с = федкин + %lld = %s\н" б с;
зов гойда "указатель на федкин = %lld\н" федкин;
зов гойда "указатель на с      = %lld\н" с;
пусть _ равно зов гойда "федкин это %s\н" федкин;

пусть гоооооол будет "ГОООООООООООООООООООООООЛ\н";
пусть _ равно зов гойда гоооооол;

	норм конструкция
всеравно зов гойда гоооооол;

зов гойда "%lld\н" 123; 
зов гойда "abcdefg %s %s %s %s %s %s\н" + 3 "1" "2" "3" "4" "5" "6";
зов гойда "abcdefg %s %s %s %s %s\н" + 3 "1" "2" "3" "4" "5";
зов гойда "abcdefg %s %s %s %s\н" + 3 "1" "2" "3" "4";
зов гойда "abcdefg %s %s %s\н" + 3 "1" "2" "3";
зов гойда "%lld\н" 123; 


зов гойда "%lld\н" 123; 
зов гойда "сво\н";
зов гойда "abcdefg %s %s %s %s %s %s\н" + 3 "1" "2" "3" "4" "5" "6";
зов гойда "абвгджз\н" + 3;

зов гойда "%lld\н" 123; 

зов начертать "%lld" "1234";
зов гойда "%lld\н" 123;
зов начертать "%lld" "1234";  

тут неясное поведение с scanf по причине того что это указатель на строку
с нормальным указателем на dq все работает

зов начертать "%lld" "1234";
зов начертать "%lld" "1234";
зов начертать "%lld" "1234";
зов начертать "%lld" "1234";

зов гойда "сво\н";
зов гойда "%lld\н" 123; 
зов гойда "%lld\н" 123; 
зов гойда "%lld\н" 123; 

(123 + 123.123) - 123.123123 / 0;
зов гойда "сво\н";
зов гойда "сво\н";
зов гойда "сво\н";
зов гойда "сво\н";