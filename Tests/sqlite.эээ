из "msvcrt" черпать "printf" как "гойда" некого;
из "msvcrt" черпать "scanf" как "начертать" некого;
Ё https://github.com/chunchunmaru0000/sqlite_in_fasm Ё
из "sqlite3" черпать "sqlite3_open" как "откр";
из "sqlite3" черпать "sqlite3_close" как "закр";
из "sqlite3" черпать "sqlite3_prepare_v2" как "подготовить";
из "sqlite3" черпать "sqlite3_step" как "шаг" типа;
из "sqlite3" черпать "sqlite3_finalize" как "конец" типа;
из "sqlite3" черпать "sqlite3_column_int64" как "кч64";
из "sqlite3" черпать "sqlite3_column_text" как "кстр";

пусть бд будет "бд.db";
зов гойда "%s\н" бд;



пусть подкл будет 0;
зов откр бд ук подкл;
зов гойда "[ БД ПОДКЛЮЧЕНА ]\н";

пусть команда будет 0;
пусть создать_таблицу будет 
"create table if not exists asdf(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	name TEXT, 
	age INTEGER
)";

зов гойда "[ НАЧАЛО ПОДГОТОВКИ КОМАНДЫ ] %s\н" создать_таблицу;
зов подготовить подкл создать_таблицу (-1) ук команда 0;
зов гойда "[ КОНЕЦ  ПОДГОТОВКИ КОМАНДЫ ]\н";

пусть шаг_вернул будет зов шаг команда;
зов гойда "[  ШАГ  ВЕРНУЛ ] %lld\н" шаг_вернул;

пусть конец_вернул будет зов конец команда;
зов гойда "[ КОНЕЦ ВЕРНУЛ ] %lld\н" конец_вернул;



пусть вставить будет 
"insert into asdf (name, age) values ('fedkin', 10), ('negr', 1123)";

зов гойда "[ НАЧАЛО ПОДГОТОВКИ КОМАНДЫ ] %s\н" вставить;
зов подготовить подкл вставить (-1) ук команда 0;
зов гойда "[ КОНЕЦ  ПОДГОТОВКИ КОМАНДЫ ]\н";

пусть шаг_вернул будет зов шаг команда;
зов гойда "[  ШАГ  ВЕРНУЛ ] %lld\н" шаг_вернул;

пусть конец_вернул будет зов конец команда;
зов гойда "[ КОНЕЦ ВЕРНУЛ ] %lld\н" конец_вернул;



зов закр подкл;
зов гойда "[ БД ЗАКРЫТА ]\н";