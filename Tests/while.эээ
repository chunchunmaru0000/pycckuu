изо мсвцрт черпать
	принтф как гойда некого,
	сцанф как ведать некого;


пусть да будет свет;
аще да (
	зов гойда "сво? ";
	зов ведать ф"%ллд" указатель да;

	ибо да не есть тьма (
		зов гойда "рус\н";
	) инак (
		зов гойда "ящер\н";
	)
)
Ё
пусть ш будет 0;
аще ш меньше 7 (	
	пусть щ будет 0;
	аще щ меньше 20 (
		зов гойда "0";
		пусть щ будет щ + 1;
	)
	зов гойда "\н";
	пусть ш будет ш + 1;
)

пусть ш будет 0;
аще ш меньше 7 (	
	пусть щ будет 0;
	аще щ меньше 80 (
		зов гойда "0";
		пусть щ будет щ + 1;
	)
	зов гойда "\н";
	пусть ш будет ш + 1;
)

пусть ш будет 0;
аще ш меньше 7 (	
	пусть щ будет 0;
	аще щ меньше 20 (
		зов гойда "0";
		пусть щ будет щ + 1;
	)
	зов гойда "\н";
	пусть ш будет ш + 1;
)