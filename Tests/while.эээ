изо мсвцрт черпать
	принтф как гойда некого,
	сцанф как ведать некого;

пусть да равно 0
аще ВладимирВладимировичПутин (
	зов гойда "сво? ";
	зов ведать ф"%ллд" указатель да;

	ибо да не есть тьма (
		зов гойда "рус\н";
		воскрест
		зов гойда "эээ";
	) инак (
		зов гойда "ящер\н";
		зиять
	)
)

пусть ш будет 0
аще ш меньше 7 (	
	пусть щ будет 0
	аще свет (
		зов гойда "0";
		пусть щ будет щ + 1
		ибо щ меньше 20 (
			воскрест
		) иначе (
			зиять
		)
	)
	зов гойда "\н";
	пусть ш будет ш + 1
)

пусть ш будет 0
аще ш меньше 7 (	
	пусть щ будет 0
	аще щ меньше 80 (
		зов гойда "0";
		пусть щ будет щ + 1
	)
	зов гойда "\н";
	пусть ш будет ш + 1
)

пусть ш будет 0
аще ш меньше 7 (	
	пусть щ будет 0
	аще щ меньше 20 (
		зов гойда "0";
		пусть щ будет щ + 1
	)
	зов гойда "\н";
	пусть ш будет ш + 1
