из "msvcrt" черпать "printf" как "гойда";
из "msvcrt" черпать "scanf" как "начертать";


зов гойда 
	"СВО ZZZ
	ZZZZZZZVZVZVZVVZVVVVVZV\нZV\н число [%llf]\н\н\н\н\т\т\тэээ\н" 
	(14 + 0.88) * 100;
зов гойда "эээ\н";

зов начертать "%lld";
зов начертать "%lld";
зов начертать "%lld";

зов гойда "%llf\н" (123 + 123.123) - 123.123123 / 0;
